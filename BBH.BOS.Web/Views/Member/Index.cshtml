<div class="modal fade" id="standardModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <!--Content-->
        <div class="modal-content">
            <!--Header-->
            <div class="modal-header white-text" style="background-color: #154771">
                <p class="heading" contenteditable="true" style="color:#fff">Admin Infomation </p>
            </div>
            <!--Body-->
            <div class="modal-body" style="padding-bottom:0px">
                <form>
                    <div class="row" style="margin-bottom:15px">
                        <!--First column-->
                        <div class="col-md-12">
                            <div class="md-form">
                                <input type="text" onfocus="ResetForm('lbErrorUserName', this.value)" id="txtUserName" name="txtUserName" placeholder="UserName" class="form-control validate">

                            </div>
                            <label id="lbErrorUserName" style="color:red; display:none" class=""></label>
                        </div>
                    </div>

                    <div class="row" style="margin-bottom:15px">
                        <!--First column-->
                        <div class="col-md-12">
                            <div class="md-form">
                                <input type="password" onfocus="ResetForm('lbErrorPassword', this.value)" id="txtPassword" name="txtPassword" placeholder="Password" class="form-control validate">

                            </div>
                            <label id="lbErrorPassword" style="color:red; display:none" class=""></label>
                        </div>
                    </div>

                    <div class="row" style="margin-bottom:15px">
                        <!--First column-->
                        <div class="col-md-12">
                            <div class="md-form">
                                <input type="text" onfocus="ResetForm('lbErrorEmail', this.value)" id="txtEmail" name="txtEmail" placeholder="Email" class="form-control validate">

                            </div>
                            <label id="lbErrorEmail" style="color:red; display:none" class=""></label>
                        </div>
                    </div>

                    <div class="row" style="margin-bottom:15px">
                        <!--First column-->
                        <div class="col-md-12">
                            <div class="md-form">
                                <input type="text" onfocus="ResetForm('lbErrorFullName', this.value)" id="txtFullName" name="txtFullName" placeholder="FullName" class="form-control validate">

                            </div>
                            <label id="lbErrorFullName" style="color:red; display:none" class=""></label>
                        </div>
                    </div>

                    <div class="row" style="margin-bottom:15px">
                        <!--First column-->
                        <div class="col-md-12">
                            <div class="md-form">
                                <input type="text" id="txtMobile" style="text-align:left" onfocus="ResetForm('lbErrorMobie', this.value)" name="txtMobile" placeholder="Mobile" onkeypress="return functionx(event)" class="form-control">
                            </div>
                            <label id="lbErrorMobie" style="color:red; display:none" class=""></label>
                        </div>
                    </div>

                    <div class="row" style="margin-bottom:15px">
                        <!--First column-->
                        <div class="col-md-12">
                            <div class="md-form">
                                <input type="text" id="txtAddress" name="txtAddress" placeholder="Address" class="form-control">
                            </div>
                        </div>
                    </div>

                </form>
                <br>

            </div>
            <!--Footer-->
