<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service name="BBH.BOS.Domain.Interfaces.IMemberService">
                <endpoint address="" binding="basicHttpBinding" contract="BBH.BOS.Domain.Interfaces.IIMemberService">
                    <identity>
                        <dns value="localhost" />
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/BBH.BOS.Domain.Interfaces/IMemberService/" />
                    </baseAddresses>
                </host>
            </service>
            <service name="BBH.BOS.Domain.Interfaces.ITransactionWalletService">
                <endpoint address="" binding="basicHttpBinding" contract="BBH.BOS.Domain.Interfaces.ITransactionWalletService">
                    <identity>
                        <dns value="localhost" />
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/BBH.BOS.Domain.Interfaces/ITransactionWalletService/" />
                    </baseAddresses>
                </host>
            </service>
          <service name="BBH.BOS.Domain.Interfaces.IPackageService">
            <endpoint address="" binding="basicHttpBinding" contract="BBH.BOS.Domain.Interfaces.IPackageService">
              <identity>
                <dns value="localhost" />
              </identity>
            </endpoint>
            <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
            <host>
              <baseAddresses>
                <add baseAddress="http://localhost:8733/Design_Time_Addresses/BBH.BOS.Domain.Interfaces/IPackageService/" />
              </baseAddresses>
            </host>
          </service>

        </services>
    </system.serviceModel>
</configuration>
